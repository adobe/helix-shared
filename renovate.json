{
  "extends": [
    "config:base",
    ":semanticCommits",
    ":autodetectPinVersions"
  ],
  "timezone": "Europe/Zurich",
  "branchPrefix": "renovate-",
  "packageRules": [
    {
      "packageNames": [
        "hast-util-to-string",
        "mdast-util-gfm-footnote",
        "mdast-util-gfm-strikethrough",
        "mdast-util-gfm-table",
        "mdast-util-gfm-task-list-item",
        "micromark-extension-gfm-footnote",
        "micromark-extension-gfm-strikethrough",
        "micromark-extension-gfm-table",
        "micromark-extension-gfm-tagfilter",
        "micromark-extension-gfm-task-list-item",
        "micromark-util-character",
        "micromark-util-combine-extensions",
        "micromark-util-symbol",
        "unist-util-find"
      ],
      "allowedVersions": "<2.0.0"
    },
    {
      "packageNames": ["mdast-util-phrasing"],
      "allowedVersions": "<4.0.0"
    },
    {
      "packageNames": ["unist-util-visit"],
      "allowedVersions": "<5.0.0"
    },
    {
      "packageNames": ["hast-util-select"],
      "allowedVersions": "<6.0.0"
    },
    {
      "packageNames": ["hastscript", "unist-util-inspect"],
      "allowedVersions": "<8.0.0"
    },
    {
      "packageNames": ["hast-util-raw", "hast-util-to-html"],
      "allowedVersions": "<9.0.0"
    },
    {
      "packageNames": ["unified"],
      "allowedVersions": "<11.0.0"
    },
    {
      "packageNames": ["mdast-util-to-hast"],
      "allowedVersions": "<13.0.0"
    },

    {
      "packageNames": ["quick-lru"],
      "allowedVersions": "<6.0.0"
    },
    {
      "packageNames": ["callsites"],
      "allowedVersions": "<4.0.0"
    },
    {
      "packageNames": ["escape-string-regexp"],
      "allowedVersions": "<5.0.0"
    },
    {
      "packageNames": ["p-limit"],
      "allowedVersions": "<4.0.0"
    },
    {
      "packageNames": ["circleci/node"],
      "allowedVersions": "<15"
    },
    {
      "packageNames": ["cimg/node"],
      "allowedVersions": "<19"
    },
    {
      "packageNames": ["@aws-sdk/client-s3"],
      "allowedVersions": "!/3\\.29\\.0/"
    },
    {
      "packageNames": ["@adobe/helix-cli"],
      "allowedVersions": "<14"
    },
    {
      "groupName": "adobe fixes",
      "updateTypes": ["patch", "pin", "digest", "minor"],
      "automerge": true,
      "packagePatterns": ["^@adobe/"],
      "schedule": ["at any time"]
    },
    {
      "groupName": "adobe major",
      "updateTypes": ["major"],
      "packagePatterns": ["^@adobe/"],
      "automerge": false,
      "schedule": ["at any time"]
    },
    {
      "groupName": "external fixes",
      "updateTypes": ["patch", "pin", "digest", "minor"],
      "automerge": true,
      "schedule": ["after 2pm on Saturday"],
      "packagePatterns": ["^.+"],
      "excludePackagePatterns": ["^@adobe/"]
    },
    {
      "groupName": "external major",
      "updateTypes": ["major"],
      "automerge": false,
      "packagePatterns": ["^.+"],
      "excludePackagePatterns": ["^@adobe/"],
      "schedule": ["after 2pm on Monday"]
    },
    {
      "datasources": ["orb"],
      "updateTypes": ["patch", "minor"],
      "automerge": true
    }
  ],
  "encrypted": {
    "npmToken": ""
  }
}
