{
  "name": "@adobe/helix-shared",
  "version": "7.21.0",
  "description": "Shared modules of the Helix Project",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "semantic-release": "semantic-release",
    "docs": "npx jsonschema2md -d src/schemas -o docs -x docs -f yaml &&  npx jsdoc2md -c .jsdoc.json --files src/*.js > docs/API.md",
    "test": "nyc mocha",
    "test-ci": "nyc mocha --reporter xunit --reporter-options output=./junit/test-results.xml && codecov",
    "lint": "eslint .",
    "junit": "mocha --exit -R mocha-junit-reporter"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-shared.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-shared/issues"
  },
  "homepage": "https://github.com/adobe/helix-shared#readme",
  "dependencies": {
    "@adobe/helix-shared-utils": "*",
    "@adobe/helix-shared-wrap": "*",
    "@adobe/helix-shared-git": "*",
    "@adobe/helix-fetch": "2.1.6",
    "@adobe/helix-log": "5.0.0",
    "ajv": "8.1.0",
    "ajv-formats": "2.0.2",
    "ferrum": "1.9.0",
    "fs-extra": "9.1.0",
    "lru-cache": "6.0.0",
    "object-hash": "2.1.1",
    "uri-js": "4.4.1",
    "yaml": "1.10.0"
  },
  "devDependencies": {
    "@adobe/helix-testutils": "0.4.3",
    "@pollyjs/adapter-node-http": "5.1.0",
    "@pollyjs/core": "5.1.0",
    "@pollyjs/persister-fs": "5.0.0",
    "jsdom": "16.4.0",
    "nock": "13.0.9"
  }
}
